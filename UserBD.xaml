<Window x:Class="Diplom1.UserBD"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diplom1"
        mc:Ignorable="d"
        Title="UserBD" Height="450" Width="800" WindowStartupLocation ="CenterScreen">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/back2.png"/>
        </Grid.Background>
        <DataGrid x:Name="UserGrid" HorizontalAlignment="Center" Height="414" VerticalAlignment="Center" Width="780" Loaded="UserList" >
            <DataGrid.Background>
                <ImageBrush/>
            </DataGrid.Background>
        </DataGrid>

    </Grid>
</Window>
